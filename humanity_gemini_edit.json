{
    "key_definitions": {
        "sexual_preference": {
            "Attraction to Male (AM)": "Degree of attraction to individuals identifying as male (0=none, 10=exclusive).",
            "Attraction to Female (AF)": "Degree of attraction to individuals identifying as female (0=none, 10=exclusive).",
            "Attraction to Other Genders/Non-Binary (AONB)": "Degree of attraction to non-binary/other gender identities (0=none, 10=exclusive).",
            "Romantic vs. Sexual Distinction (RSD)": "Degree to which romantic and sexual attraction are differentiated (0=no distinction, 10=clear distinction).",
            "Fluidity/Change Over Time (FCT)": "Degree of stated or implied fluidity in preference (0=fixed, 10=highly fluid)."
        },
        "gender_identity": {
            "Identification with Maleness (IM)": "Degree of identification with concepts, roles, or expressions traditionally associated with 'maleness' (0=none, 10=exclusive).",
            "Identification with Femaleness (IF)": "Degree of identification with concepts, roles, or expressions traditionally associated with 'femaleness' (0=none, 10=exclusive).",
            "Identification with Androgyny/Non-Binary (IANB)": "Degree of identification with gender identities that blend elements or exist outside male/female (0=none, 10=exclusive).",
            "Gender Expression (GE)": "Outward presentation of gender (0=very feminine, 5=androgynous/not specified, 10=very masculine)."
        },
        "political_beliefs": {
            "Economic Ideology (EI)": "From 0 (extreme socialism/communism) to 10 (extreme capitalism/laissez-faire).",
            "Social/Cultural Ideology (SCI)": "From 0 (progressive/liberal) to 10 (conservative/traditional).",
            "Authoritarianism vs. Libertarianism (AVL)": "From 0 (authoritarian) to 10 (libertarian).",
            "Environmentalism/Sustainability (ES)": "From 0 (low priority/disregard) to 10 (high priority/strong advocacy).",
            "Foreign Policy/Internationalism vs. Nationalism (FIN)": "From 0 (strong nationalism/isolationism) to 10 (strong internationalism/global cooperation)."
        },
        "relationship_goals": {
            "Commitment Level (CL)": "From 0 (casual/short-term) to 10 (long-term/lifelong commitment).",
            "Monogamy vs. Non-Monogamy (MVNM)": "From 0 (strict monogamy) to 10 (highly non-monogamous/polyamorous).",
            "Family/Children Orientation (FCO)": "From 0 (no interest in family/children) to 10 (strong desire for family/children).",
            "Independence vs. Interdependence (IVI)": "From 0 (strong interdependence/enmeshment) to 10 (strong independence/autonomy).",
            "Shared Activities/Interests (SAI)": "From 0 (low importance of shared activities) to 10 (high importance of shared activities).",
            "Emotional Intimacy Depth (EID)": "From 0 (low emotional intimacy) to 10 (deep emotional intimacy)."
        }
    },
